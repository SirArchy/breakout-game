{
  "name": "react-brickbreaker",
  "version": "0.4.0",
  "private": false,
  "homepage": "https://rodionchachura.github.io/breakout-game",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "copyfiles": "^2.4.1",
    "gh-pages": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lib": "babel src --out-dir lib && npm run copy:css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "copy:css": "copyfiles -u 1 src/**/*.css lib"
